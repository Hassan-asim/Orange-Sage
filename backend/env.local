# App
APP_ENV=development
APP_HOST=0.0.0.0
APP_PORT=8000
SECRET_KEY=your_super_secret_key_replace_me_with_strong_random_key

# Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=orange_sage
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
DATABASE_URL=postgresql+psycopg2://postgres:postgres@localhost:5432/orange_sage
ASYNC_DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/orange_sage

# CORS
BACKEND_CORS_ORIGINS=["http://localhost:5173", "http://localhost:3000"]

# LLM - Replace with your actual API keys
OPENAI_API_KEY=sk-your-openai-key-here
GEMINI_API_KEY=AIzaSyC-your-gemini-key-here
LLM_MODEL=gpt-4o

# Celery
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2

# S3/MinIO
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_BUCKET=orange-sage

# Docker Registry for agents
DOCKER_REGISTRY_URL=https://index.docker.io/v1/
AGENT_IMAGE_PREFIX=orange-sage/agent-
AGENT_MAX_ITERATIONS=200
